<specbattlemgr.def>
  <Properties>
    <bp>
      <Type>	PYTHON	</Type>
      <Flags>	BASE	</Flags>
    </bp>
    <pdata>
      <Type>	PYTHON	</Type>
      <Flags>	BASE	</Flags>
      <Persistent>	true	</Persistent>
      <DatabaseLength>	1024000	</DatabaseLength>
    </pdata>
    <name>
      <Type>	STRING	</Type>
      <Flags>	BASE	</Flags>
      <Persistent>	true	</Persistent>
      <DatabaseLength>	16	</DatabaseLength>
      <Identifier>	true	</Identifier>
    </name>
  </Properties>
  <Implements>
    <Interface>	InterclusterSender	</Interface>
  </Implements>
  <BaseMethods>
    <scheduleBattle>
      <Arg>	DB_ID	</Arg>
      <Arg>	STRING	</Arg>
      <Arg>	DB_ID	</Arg>
      <Arg>	PYTHON	</Arg>
    </scheduleBattle>
    <joinBattle>
      <Arg>	UINT64	</Arg>
      <Arg>	MAILBOX	</Arg>
      <Arg>	DB_ID	</Arg>
      <Arg>	DB_ID	</Arg>
    </joinBattle>
    <onUnitMgrReady>
      <Arg>	DB_ID	</Arg>
      <Arg>	DB_ID	</Arg>
      <Arg>	MAILBOX	</Arg>
      <Arg>	PYTHON	</Arg>
      <Arg>	BOOL	</Arg>
      <Arg>	PYTHON	</Arg>
    </onUnitMgrReady>
    <onUnitMgrPublish>
      <Arg>	MAILBOX	</Arg>
      <Arg>	DB_ID	</Arg>
      <Arg>	DB_ID	</Arg>
    </onUnitMgrPublish>
    <onUnitMgrUpdate>
      <Arg>	DB_ID	</Arg>
      <Arg>	DB_ID	</Arg>
      <Arg>	PYTHON	</Arg>
    </onUnitMgrUpdate>
    <onUnitMgrCleanup>
      <Arg>	DB_ID	</Arg>
      <Arg>	DB_ID	</Arg>
    </onUnitMgrCleanup>
    <onArenaCreated>
      <Arg>	MAILBOX	</Arg>
      <Arg>	UINT64	</Arg>
      <Arg>	UINT8	</Arg>
      <Arg>	PYTHON	</Arg>
      <Arg>	FLOAT32	</Arg>
      <Arg>	PYTHON	</Arg>
    </onArenaCreated>
    <onArenaFailure>
      <Arg>	DB_ID	</Arg>
      <Arg>	INT16	</Arg>
    </onArenaFailure>
    <onArenaFinished>
      <Arg>	DB_ID	</Arg>
      <Arg>	INT16	</Arg>
      <Arg>	PYTHON	</Arg>
      <Arg>	PYTHON	</Arg>
      <Arg>	UINT64	</Arg>
    </onArenaFinished>
    <setDevMode>
      <Arg>	BOOL	</Arg>
    </setDevMode>
  </BaseMethods>
</specbattlemgr.def>
